{
  "name": "dravencms/base",
  "type": "dravencms-package",
  "description": "DravenCMS Base",
  "keywords": [
    "dravencms",
    "base"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "nette/nette": "2.4",
    "janmarek/webloader": "dev-master",
    "joseki/webloader-filters": "~1.0",
    "linkorb/jsmin-php": ">=1.0.0",
    "leafo/scssphp": "dev-master",
    "natxet/CssMin": "dev-master",
    "salamek/raven-nette": "dev-master",
    "kdyby/doctrine": "^3.1.2",
    "rixxi/gedmo": "^1.0",
    "zenify/doctrine-migrations": "^2.3",
    "doctrine/data-fixtures": "^1.1",
    "dravencms/recaptcha": "@dev",
    "dravencms/packager": "dev-master",
    "o5/grido": "dev-master",
    "dravencms/paginator": "@dev"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "base": "Dravencms\\Base\\DI\\BaseExtension"
        },
        "services": {
          "route": {
            "class": "Dravencms\\Base\\RouterFactory"
          },
          "router": "@Dravencms\\Base\\RouterFactory::createRouter"
        }
      }
    }
  }
}