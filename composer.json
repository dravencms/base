{
  "name": "dravencms/base",
  "type": "dravencms-package",
  "description": "DravenCMS Base",
  "keywords": [
    "dravencms",
    "base"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    
    "nette/application": "^2.4.4",
    "nette/bootstrap": "^2.4.3",
    "nette/caching": "^2.5",
    "nette/di": "^2.4",
    "nette/finder": "^2.4",
    "nette/forms": "^2.4",
    "nette/http": "^2.4",
    "nette/mail": "^2.4",
    "nette/robot-loader": "^2.4 || ^3.0",
    "nette/safe-stream": "^2.3",
    "nette/security": "^2.4",
    "nette/utils": "^2.4",
    "latte/latte": "^2.4",
    "tracy/tracy": "^2.4",
    
    "janmarek/webloader": "dev-master",
    "joseki/webloader-filters": "~1.0",
    "linkorb/jsmin-php": ">=1.0.0",
    "leafo/scssphp": "^v0.7",
    "natxet/CssMin": "^3.0.4",
    "salamek/raven-nette": "^v1.3.3",
    "kdyby/doctrine": "^v3.3.x-dev",
    "kdyby/events": "^v3.1",
    "zenify/doctrine-migrations": "^v2.3.5",
    "doctrine/data-fixtures": "^1.1",
    "dravencms/recaptcha": "^v1.0",
    "dravencms/packager": "^v1.0",
    "o5/grido": "dev-master",
    "dravencms/paginator": "^v1.0",
    "salamek/gedmo": "^1.0"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "base": "Dravencms\\Base\\DI\\BaseExtension"
        },
        "services": {
          "route": {
            "class": "Dravencms\\Base\\RouterFactory"
          },
          "router": "@Dravencms\\Base\\RouterFactory::createRouter"
        }
      }
    }
  }
}
